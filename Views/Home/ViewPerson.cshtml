@model Person

@{
    ViewData["Title"] = "Information List";
}
<table class = "table table-striped table-inverse">
    <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Middle Name</th>
        <th>Last Name</th>
    </tr>
    @if(Model != null){        
        <tr>
            <td>@Model.Id</td>
            <td>@Model.FirstName</td>
            <td>@Model.MiddleName</td>
            <td>@Model.LastName</td>
        </tr>
    }
    else{
        <tr>
            <td colspan="4" class="text-center">Not Found</td>
        </tr>
    }
</table>
