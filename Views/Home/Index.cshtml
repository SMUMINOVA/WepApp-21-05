@model IEnumerable<Person>

@{
    ViewData["Title"] = "Home Page";
}
<table class = "table table-striped table-inverse">
    <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Middle Name</th>
        <th>Last Name</th>
    </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.FirstName</td>
                <td>@item.MiddleName</td>
                <td>@item.LastName</td>
            </tr>
        }
</table>
