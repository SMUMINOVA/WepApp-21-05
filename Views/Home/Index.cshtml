@{
    ViewData["Title"] = "Home Page";
}
<table>
    <title>
        <tr>
            <th>Id</th>
            <th>First Name</th>
            <th>Middle Name</th>
            <th>Last Name</th>
        </tr>
    </title>
    <tr>
        @foreach (var item in Model)
        {
            <td>@item.Id</td>
            <td>@item.FirstName</td>
            <td>@item.MiddleName</td>
            <td>@item.LastName</td>
        }
    </tr>
</table>
